pipeline {
    agent any
    stages {
        stage('test') {
            	steps {
		        sleep(time:2,unit:"MINUTES")
               		sh 'echo $(git log -n 1 --pretty=format:"%H")'
            }
	stage('merge'){
	        steps{
		    retry(2){
		        sh 'STATE=`git rev-list --left-right --count origin/master...$(git branch --show-current) | awk "{print $1}"`'
			
                    }
		}
            }
        }
    }
    post { 
        success { 
            echo 'successul build'
        }
    }
}

